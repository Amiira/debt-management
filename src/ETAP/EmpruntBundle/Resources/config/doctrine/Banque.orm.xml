<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ETAP\EmpruntBundle\Entity\Banque" table="BANQUE">
    <id name="refbanque" type="integer" column="REFBANQUE">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nom" type="string" column="NOM" length="20" nullable="true"/>
    <many-to-many field="refdemande" target-entity="Demande" inversed-by="refbanque">
      <join-table name="banquedemande">
        <join-columns>
          <join-column name="REFBANQUE" referenced-column-name="REFBANQUE"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="REFDEMANDE" referenced-column-name="REFDEMANDE"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
